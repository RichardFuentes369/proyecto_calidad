#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/richard/projects/uts/calidad/node_modules/.pnpm/typeorm@0.3.22_mongodb@6.16.0_socks@2.8.4__mysql2@3.14.1_reflect-metadata@0.1.14_ts-nod_4621cb6f262f68445366f755938ccb1a/node_modules/typeorm/node_modules:/home/richard/projects/uts/calidad/node_modules/.pnpm/typeorm@0.3.22_mongodb@6.16.0_socks@2.8.4__mysql2@3.14.1_reflect-metadata@0.1.14_ts-nod_4621cb6f262f68445366f755938ccb1a/node_modules:/home/richard/projects/uts/calidad/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/richard/projects/uts/calidad/node_modules/.pnpm/typeorm@0.3.22_mongodb@6.16.0_socks@2.8.4__mysql2@3.14.1_reflect-metadata@0.1.14_ts-nod_4621cb6f262f68445366f755938ccb1a/node_modules/typeorm/node_modules:/home/richard/projects/uts/calidad/node_modules/.pnpm/typeorm@0.3.22_mongodb@6.16.0_socks@2.8.4__mysql2@3.14.1_reflect-metadata@0.1.14_ts-nod_4621cb6f262f68445366f755938ccb1a/node_modules:/home/richard/projects/uts/calidad/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli-ts-node-esm.js" "$@"
else
  exec node  "$basedir/../../cli-ts-node-esm.js" "$@"
fi
